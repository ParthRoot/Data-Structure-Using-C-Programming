# include<stdio.h>
#include<conio.h>
void main()
{
    int a[50], i, j, n, temp;

    printf("Enter the size of array\n");
    scanf("%d", &n);

    printf("Enter Array Elements\n");
    for (i = 0; i < n; i++)
    {
        scanf("%d", &a[i]);
    }

    for(i=0;i<n-1;i++)
    {
        for(j=0; j<n; j++)
        {
            if(a[j] > a[j+1])
            {
                temp = a[j];
                a[j] = a[j+1];
                a[j+1] = temp;
            }
        }
    }
    printf("Sorted Array\n");
    for(i=0;i<n;i++){
        printf("%d\n", a[i]);
    }
    getch();
}
