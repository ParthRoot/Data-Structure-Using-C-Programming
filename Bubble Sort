# include<stdio.h>
int main()
{
    int a[50], n, i, j;
    printf("Enter the length of array\n");
    scanf("%d", &n);

    printf("Enter the array element\n");
    for (i = 0; i<n; i++)
    {
        scanf("%d", &a[i]);
    }

    for(i=0; i < n-1; i++)
    {
        for(j=0; j < n-i-1; j++)
        {
            if (a[j] > a[j + 1])
            {
                int swap = a[j];
                a[j] = a[j+1];
                a[j+1] = swap;
            }
        }
    }

    printf("Sort the array:\n");
    for (i = 0; i<n; i++)
    {
        printf("%d\n", a[i]);
    }
    return 0;
}
